{
  "name": "universal-pdf-viewer",
  "version": "1.0.0",
  "description": "A beautiful, feature-rich PDF viewer plugin that works with any framework",
  "main": "dist/pdf-viewer.js",
  "module": "dist/pdf-viewer.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "serve": "http-server -p 8080",
    "test": "jest",
    "lint": "eslint src/**/*.{js,ts}",
    "format": "prettier --write src/**/*.{js,ts,css}"
  },
  "keywords": [
    "pdf",
    "viewer",
    "pdf-viewer",
    "react",
    "vue",
    "angular",
    "vanilla",
    "framework-agnostic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pdfjs-dist": "^3.11.174"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ]
}