{
  "name": "@ldesign/pdf-vue",
  "version": "2.0.0",
  "description": "Vue 3 components for PDF viewer",
  "keywords": [
    "pdf",
    "vue",
    "vue3",
    "pdf-viewer",
    "vue-component"
  ],
  "author": "",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./es/PDFViewer.vue.css": "./es/PDFViewer.vue.css",
    "./lib/index.css": "./lib/index.css"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/pdf-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/compiler-sfc": "^3.3.13",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue": "^3.3.13",
    "vue-tsc": "^1.8.27"
  },
  "publishConfig": {
    "access": "public"
  }
}